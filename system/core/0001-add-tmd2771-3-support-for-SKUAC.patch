 rootdir/init.rc |    3 +++
 1 files changed, 3 insertions(+), 0 deletions(-)

diff --git a/rootdir/init.rc b/rootdir/init.rc
index 760838d..dc9b41f 100644
--- a/rootdir/init.rc
+++ b/rootdir/init.rc
@@ -356,6 +356,9 @@ on boot
     class_start core
     class_start main
 
+# TMD2771-3
+    chmod 0666 /dev/tmd27713
+
 on nonencrypted
     class_start late_start
 
