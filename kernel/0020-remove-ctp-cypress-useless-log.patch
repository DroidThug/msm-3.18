From 4f2acd040a75d9f0258d6d11866356e24bbdf33b Mon Sep 17 00:00:00 2001
From: lnxbuild <lnxbuild@localhost>
Date: Fri, 13 Sep 2013 16:36:34 +0800
Subject: [PATCH 20/22] remove ctp cypress useless log

Change-Id: I99efef0db4b4d880904a5832bead4780080f7ae6
---
 drivers/input/touchscreen/cypress/cyttsp4_debug.c  |    6 +++---
 .../input/touchscreen/cypress/cyttsp4_mt_common.c  |    2 +-
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/drivers/input/touchscreen/cypress/cyttsp4_debug.c b/drivers/input/touchscreen/cypress/cyttsp4_debug.c
index 18e56da..5778b79 100644
--- a/drivers/input/touchscreen/cypress/cyttsp4_debug.c
+++ b/drivers/input/touchscreen/cypress/cyttsp4_debug.c
@@ -106,7 +106,7 @@ static void cyttsp4_pr_buf_op_mode(struct device *dev, u8 *pr_buf,
 					si->btn_rec_data[i]);
 		total_size += num_btns * si->si_ofs.btn_rec_size + 1;
 	}
-	pr_info("%s=%s%s\n", "cyttsp4_OpModeData", pr_buf,
+	pr_debug("%s=%s%s\n", "cyttsp4_OpModeData", pr_buf,
 			total_size <= max ? "" : CY_PR_TRUNCATED);
 }
 
@@ -227,7 +227,7 @@ static int cyttsp4_xy_worker(struct cyttsp4_debug_data *dd)
 	}
 
 	/* Interrupt */
-	pr_info("Interrupt(%u)\n", dd->interrupt_count);
+	pr_debug("Interrupt(%u)\n", dd->interrupt_count);
 
 	if (formated_output)
 		cyttsp4_debug_formated(dev, dd->pr_buf, si, num_cur_rec);
@@ -250,7 +250,7 @@ static int cyttsp4_xy_worker(struct cyttsp4_debug_data *dd)
 				150, "cyttsp4_sensor_monitor");
 	}
 
-	pr_info("\n");
+	pr_debug("\n");
 
 	dev_vdbg(dev, "%s: done\n", __func__);
 
diff --git a/drivers/input/touchscreen/cypress/cyttsp4_mt_common.c b/drivers/input/touchscreen/cypress/cyttsp4_mt_common.c
index 4913ea4..1e483ad 100644
--- a/drivers/input/touchscreen/cypress/cyttsp4_mt_common.c
+++ b/drivers/input/touchscreen/cypress/cyttsp4_mt_common.c
@@ -173,7 +173,7 @@ static void cyttsp4_get_mt_touches(struct cyttsp4_mt_data *md, int num_cur_rec)
 		mt_sync_count++;
 
 cyttsp4_get_mt_touches_pr_tch:
-			printk("x y z\n");
+			pr_debug("x y z\n");
 #if 0
 		if (IS_TTSP_VER_GE(si, 2, 3))
 			dev_dbg(dev,
