From a9982b4e28d4880f1b3042b12b8ff3c4587679b0 Mon Sep 17 00:00:00 2001
From: lnxbuild <lnxbuild@localhost>
Date: Fri, 13 Sep 2013 12:23:20 +0800
Subject: [PATCH 17/22] add battery data dts

Change-Id: I3f0e3db134f50c2f62abe17bbcf559b8c1ec902f
---
 arch/arm/boot/dts/msm8610-qrd.dtsi |   39 ++++++++++++++++++++++++++++++++++++
 arch/arm/boot/dts/msm8610-v1.dtsi  |    4 ---
 2 files changed, 39 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/msm8610-qrd.dtsi b/arch/arm/boot/dts/msm8610-qrd.dtsi
index 08f2317..99748e3 100644
--- a/arch/arm/boot/dts/msm8610-qrd.dtsi
+++ b/arch/arm/boot/dts/msm8610-qrd.dtsi
@@ -507,3 +507,42 @@
 &android_usb {
 	qcom,android-usb-cdrom;
 };
+
+/ {
+        qrd_batterydata: qcom,battery-data {
+                qcom,rpull-up-kohm = <100>;
+                qcom,vref-batt-therm = <1800000>;
+
+                /include/ "batterydata-qrd-4v2-1300mah.dtsi"
+        };
+};
+
+&pm8110_bms {
+        status = "ok";
+        qcom,battery-data = <&qrd_batterydata>;
+};
+
+&pm8110_chg {
+        qcom,battery-data = <&qrd_batterydata>;
+};
+
+&soc {
+    sound {
+        qcom,model = "msm8x10-skuac-snd-card";
+
+        qcom,audio-routing =
+            "RX_BIAS", "MCLK",
+            "INT_LDO_H", "MCLK",
+            "Lineout amp", "LINEOUT",
+            "MIC BIAS Internal1", "Handset Mic",
+            "MIC BIAS Internal2", "Headset Mic",
+            "AMIC1", "MIC BIAS Internal1",
+            "AMIC2", "MIC BIAS Internal2";
+
+        qcom,ext-spk-amp-gpio = <&msmgpio 92 0x0>;
+    };
+
+        qcom,dsi_v2_hx8379a_wvga_video {
+                qcom,cont-splash-enabled;
+        };
+};
diff --git a/arch/arm/boot/dts/msm8610-v1.dtsi b/arch/arm/boot/dts/msm8610-v1.dtsi
index 9d8c411..74ae6a2 100644
--- a/arch/arm/boot/dts/msm8610-v1.dtsi
+++ b/arch/arm/boot/dts/msm8610-v1.dtsi
@@ -19,7 +19,3 @@
 /include/ "msm8610.dtsi"
 /include/ "msm8610-v1-pm.dtsi"
 
-/ {
-	qcom,msm-id = <147 0>, <165 0>, <161 0>, <162 0>,
-			<163 0>, <164 0>, <166 0>;
-};
-- 
